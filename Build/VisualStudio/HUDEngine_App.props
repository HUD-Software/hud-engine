<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
   <!-- Display name -->
  <PropertyGroup>
    <_PropertySheetDisplayName>HUDEngine App</_PropertySheetDisplayName>
  </PropertyGroup>
  
  <PropertyGroup Label="Configuration">
    <PlatformToolset>v142</PlatformToolset>
    <ConfigurationType>Application</ConfigurationType>
  </PropertyGroup>
  
  <!-- Global ClCompile --> 
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(ProjectDir)\Implementation;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>

</Project>